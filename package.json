{
  "name": "ng2-pm",
  "version": "0.1.0",
  "description": "PM-Web implementation with Angular2",
  "scripts": {
    "all": "npm run ci && npm start",
    "ci": "npm install --silent && npm test && npm run build:aot",
    "syncversion": "node config/version.js",
    "clean": "rimraf aot dist",
    "compile": "ngc -p tsconfig.aot.json && rimraf trash",
    "start": "webpack-dev-server --config config/webpack.config.dev.js --inline --progress --port 3000",
    "build": "npm run clean && webpack --config config/webpack.config.prod.jit.js --progress --profile --bail",
    "build:aot": "npm run clean && npm run compile && webpack --config config/webpack.config.prod.aot.js --progress --profile --bail",
    "api": "json-server api/db.json --routes api/routes.json --port 3001",
    "test": "karma start config/karma.conf.js",
    "test:watch": "karma start config/karma.conf.js --no-single-run --auto-watch",
    "postversion": "npm run syncversion"
  },
  "author": "froko",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/froko/ng2-pm.git"
  },
  "dependencies": {
    "@angular/common": "2.2.4",
    "@angular/compiler": "2.2.4",
    "@angular/compiler-cli": "2.2.4",
    "@angular/core": "2.2.4",
    "@angular/forms": "2.2.4",
    "@angular/http": "2.2.4",
    "@angular/platform-browser": "2.2.4",
    "@angular/platform-browser-dynamic": "2.2.4",
    "@angular/platform-server": "2.2.4",
    "@angular/router": "3.2.4",
    "@angular/upgrade": "2.2.4",
    "core-js": "2.4.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@types/core-js": "0.9.35",
    "@types/jasmine": "2.5.38",
    "@types/node": "6.0.48",
    "angular2-router-loader": "0.3.4",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "2.2.4",
    "bootstrap-loader": "^2.0.0-beta.15",
    "bootstrap-sass": "^3.3.7",
    "codelyzer": "2.0.0-beta.3",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "font-awesome": "^4.7.0",
    "font-awesome-sass-loader": "^1.0.2",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "imports-loader": "0.7.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine": "2.5.2",
    "jquery": "^2.2.3",
    "json-server": "^0.9.3",
    "karma": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.1",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-istanbul": "0.2.1",
    "karma-source-map-support": "1.2.0",
    "karma-webpack": "1.8.0",
    "ng2-bootstrap": "^1.1.16",
    "node-sass": "3.13.0",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "2.1.14",
    "raw-loader": "0.5.1",
    "remap-istanbul": "0.7.0",
    "replace-in-file": "2.0.3",
    "resolve-url-loader": "1.6.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "tslint": "4.0.2",
    "tslint-loader": "3.3.0",
    "typescript": "2.0.10",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.26",
    "webpack-dev-server": "2.1.0-beta.8",
    "webpack-merge": "1.1.1"
  }
}
